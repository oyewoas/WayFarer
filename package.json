{
  "name": "WayFarer",
  "version": "1.0.0",
  "description": "WayFarer is a public bus transportation booking server, developed the back-end API with NodeJs",
  "main": "index.js",
  "scripts": {
    "test": " mocha --timeout 10000",
    "cover": "istanbul cover _mocha",
    "create-tables": "babel-node ./app/db/dbConnection createAllTables",
    "drop-tables": "babel-node ./app/db/dbConnection dropAllTables",
    "setup": "npm run start && npm run create-tables",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start": "nodemon --watch . --exec babel-node -- server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyewoas/WayFarer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oyewoas/WayFarer/issues"
  },
  "homepage": "https://github.com/oyewoas/WayFarer#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.18.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "pg": "^7.11.0"
  }
}
